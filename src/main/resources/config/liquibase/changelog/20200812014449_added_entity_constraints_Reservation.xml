<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Reservation.
    -->
    <changeSet id="20200812014449-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="reservation_tour_id"
                                 baseTableName="reservation"
                                 constraintName="fk_reservation_reservation_tour_id"
                                 referencedColumnNames="id"
                                 referencedTableName="reservation_tour"/>

        <addForeignKeyConstraint baseColumnNames="reservation_hebergement_id"
                                 baseTableName="reservation"
                                 constraintName="fk_reservation_reservation_hebergement_id"
                                 referencedColumnNames="id"
                                 referencedTableName="reservation_hebergement"/>

        <addForeignKeyConstraint baseColumnNames="reservation_transfert_id"
                                 baseTableName="reservation"
                                 constraintName="fk_reservation_reservation_transfert_id"
                                 referencedColumnNames="id"
                                 referencedTableName="reservation_transfert"/>

        <addForeignKeyConstraint baseColumnNames="donneur_ordre_id"
                                 baseTableName="reservation"
                                 constraintName="fk_reservation_donneur_ordre_id"
                                 referencedColumnNames="id"
                                 referencedTableName="donneur_ordre"/>

    </changeSet>
</databaseChangeLog>
